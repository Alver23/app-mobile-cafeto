{
  "preset": "react-native",
  "roots": ["<rootDir>/src/"],
  "testMatch": ["**/+(*.)+(spec).+(ts)"],
  "collectCoverage": true,
  "collectCoverageFrom": ["**/*ts", "!**/*-mock.ts"],
  "coverageDirectory": "coverage",
  "setupFiles": ["./jest-setup-file"],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ]
}
